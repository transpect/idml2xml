  attribute SublanguageName { xsd:string }?,
  attribute Id { xsd:int }?,
  attribute HyphenationVendor { xsd:string }?,
  attribute SpellingVendor { xsd:string }?,
  element Properties {
    element Label { element KeyValuePair { KeyValuePair_TypeDef }* 
    }?
  }
  ?
}

