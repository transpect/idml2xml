    element PageNumberStyle { 
      (enum_type, PageNumberStyle_EnumValue ) |
      (string_type, xsd:string )
    }?&
    element Label { element KeyValuePair { KeyValuePair_TypeDef }* 
    }?
  }
  ?
}

DocumentUser_Object = element DocumentUser {
